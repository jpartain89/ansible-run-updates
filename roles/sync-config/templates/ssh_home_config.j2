# {{ ansible_managed }}

{% for sect in ssh_config %}
{% if sect == 'proxyjumpers' %}
{% for host,cfg in sect.iteritems() %}
Host {{ host }}
    {% for k,v in cfg.iteritems() %}
    {{ k }} {{ v }}
    {% endfor %}
{% endfor %}
{% endif %}
{% if sect == 'servers' %}
{% for host,cfg in sect.iteritems() %}
Host {{ host }}
    {% for k,v in cfg.iteritems() %}
    {{ k }} {{ v }}
    {% endfor %}
{% endfor %}
{% endif %}
{% if sect == 'defaults' %}
{% for host,cfg in sect.iteritems() %}
Host {{ host }}
    {% for k,v in cfg.iteritems() %}
    {{ k }} {{ v }}
    {% endfor %}
{% endfor %}
{% endif %}
{% endfor %}
